@model IEnumerable<GutoriCorp.Models.BusinessViewModels.ContractViewModel>

<div class="content-heading bg-white">
    <div class="row">
        <div class="col-sm-9">
            <h4 class="m0 text-thin">Contracts</h4><small>Find, edit and create contracts</small>
        </div>
        <div class="col-sm-3 text-right hidden-xs">
            <button type="button" class="mt-sm btn btn-labeled btn-default ripple">New Contract<span class="btn-label btn-label-right"><i class="ion-plus-round"></i></span></button>
        </div>
    </div>
</div>
<div class="container-fluid">
    <!-- DATATABLE DEMO 1-->
    <div class="card">
        <div class="card-heading">Contracts</div>
        <div class="card-body">
            <table id="datatable1" class="table-datatable table table-striped table-hover mv-lg">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.contract_date)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.contract_type_id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.frequency_id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.lessee_id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.lessor_id)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.status_id)
                        </th>
                        <th>
                            Actions
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.id)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.contract_date)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.contract_type)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.frequency)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.lessee)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.lessor)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.status)
                            </td>
                            <td>
                                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
                                <a href="../ContractsVehicles/Create/cid=@item.id">Set Vehicle</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
